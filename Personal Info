using System;

class Program
{
    static void Main()
    {
        // Define personal information
        string name = "John Port";
        string birthdate = "January 1, 1990";
        string workPhoneNumber = "(123) 456-7890";
        string cellPhoneNumber = "(098) 765-4321";

        // Display the information
        Console.WriteLine("Name: " + name);
        Console.WriteLine("Birthdate: " + birthdate);
        Console.WriteLine("Work Phone Number: " + workPhoneNumber);
        Console.WriteLine("Cell Phone Number: " + cellPhoneNumber);
    }
}
